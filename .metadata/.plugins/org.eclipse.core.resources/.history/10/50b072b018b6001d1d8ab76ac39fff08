package Homework2;

public class Main {

	public static void main(String[] args) {
		
		Takim[] takim=new Takim[3];
		TakimManager takimm= new TakimManager();
		
		takim[0] = new Takim("Real Madrid");
		takim[1] = new Takim("Real Madrid");
		takim[2] = new Takim("Real Madrid");

		 Takim takim1, takim2;
		    for (int i = 0; i < takim.length - 1; i++) {
		        takim1 = takim[i];
		        for (int j = i + 1; j < takim.length; j++) {
		            takim2 = takim[j];
					takimm.macYap(takim1,takim2);
		        }
		    }
		    
		    takimm.kasaGoster(takim1);
		    takimm.puanDurumu(takim1);
		    takimm.iflasDurumu(takim1);
		    takimm.sampiyon(takim1);
		    
	}

	
}
